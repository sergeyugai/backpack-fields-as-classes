{
  "name": "sergeyugai/backpack-fields-as-classes",
  "type": "library",
  "description": "Package allows to add fields/columns/etc to Laravel Backpack while having them as classes",
  "keywords": [
    "laravel",
    "backpack",
    "fields",
    "columns"
  ],
  "homepage": "https://github.com/sergeyugai/backpack-fields-as-classes",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Yugai",
      "email": "sergey@sergeyugai.com",
      "homepage": "https://sergeyugai.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "backpack/crud": "4.1.*",
    "phpunit/phpunit": "^8",
    "orchestra/testbench": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "SergeYugai\\Laravel\\Backpack\\FieldsAsClasses\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SergeYugai\\Laravel\\Backpack\\FieldsAsClasses\\": "src",
      "Backpack\\CRUD\\Tests\\": "vendor/backpack/crud/tests",
      "Tests\\": "tests"
    },
    "files": ["vendor/backpack/crud/src/helpers.php"]
  },
  "extra": {
    "laravel": {
      "providers": [
        "SergeYugai\\Laravel\\Backpack\\FieldsAsClasses\\FieldsClassesServiceProvider"
      ]
    }
  }
}
